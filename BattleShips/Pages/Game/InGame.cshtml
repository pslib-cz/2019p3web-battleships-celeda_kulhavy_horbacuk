@page
@model BattleShips.Pages.Game.InGameModel
@{
    ViewData["Title"] = "InGame";
}

<div class="container">
    <main role="main" class="pb-3">
        <header class="game-header">
            <h1>Hrací plocha</h1>
        </header>

        @foreach (BattleShips.ViewModel.GameBoardModel gameBoard in Model.GameBoardModels)
        {
            <partial class="m-2" name="_GameBoard" model="gameBoard"/>
        }
    </main>
</div>

